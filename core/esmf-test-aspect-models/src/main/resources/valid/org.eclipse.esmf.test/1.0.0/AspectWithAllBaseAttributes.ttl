   samm:properties ( :testBoolean ) ;
   samm:operations ( ) .

:testBoolean a samm:Property ;
   samm:characteristic :BooleanTestCharacteristic .

:BooleanTestCharacteristic a samm:Characteristic ;
   samm:dataType xsd:boolean .

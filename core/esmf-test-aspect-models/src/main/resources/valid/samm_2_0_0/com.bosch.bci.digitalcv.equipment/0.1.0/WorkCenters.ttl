:WorkCenters a samm:Aspect ;
    samm:preferredName "Work Centers"@en ;
    samm:description "Aspect for work centers."@en ;
    samm:properties ( :items ) ;
    samm:operations ( ) .

:items a samm:Property ;
    samm:preferredName "Work Center List"@en ;
    samm:description "A list of work centers within a specific plant."@en ;
    samm:characteristic [
        a samm-c:SortedSet ;
        samm:dataType :WorkCenter
    ] .

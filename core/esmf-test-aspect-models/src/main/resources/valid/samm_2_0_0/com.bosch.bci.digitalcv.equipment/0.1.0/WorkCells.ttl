:WorkCells a samm:Aspect ;
    samm:preferredName "Work Cells"@en ;
    samm:description "Aspect for work cells."@en ;
    samm:properties ( :items ) ;
    samm:operations ( ) .

:items a samm:Property ;
    samm:preferredName "Work Cell List"@en ;
    samm:description "A list of work cells within a specific station."@en ;
    samm:characteristic [
        a samm-c:SortedSet ;
        samm:dataType :WorkCell
    ] .
:WorkCells a bamm:Aspect ;
    bamm:preferredName "Work Cells"@en ;
    bamm:description "Aspect for work cells."@en ;
    bamm:properties ( :items ) ;
    bamm:operations ( ) .

:items a bamm:Property ;
    bamm:preferredName "Work Cell List"@en ;
    bamm:description "A list of work cells within a specific station."@en ;
    bamm:characteristic [
        a bamm-c:SortedSet ;
        bamm:dataType :WorkCell
    ] .
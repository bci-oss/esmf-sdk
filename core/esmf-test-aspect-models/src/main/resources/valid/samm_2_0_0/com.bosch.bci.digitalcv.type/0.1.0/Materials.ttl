:Materials a samm:Aspect ;
    samm:preferredName "Product Type Bill of Material"@en ;
    samm:description "Aspect for bill of material for product."@en ;
    samm:properties ( :items ) ;
    samm:operations ( ) .

:items a samm:Property ;
    samm:preferredName "Materials"@en ;
    samm:preferredName "Materialien"@de ;
    samm:description "A list of materials sorted in chronological order."@en ;
    samm:description "Eine Liste mit chronologisch sortierten Materialien."@de ;
    samm:characteristic [
        a samm-c:SortedSet ;
        samm:dataType :Material
    ] .
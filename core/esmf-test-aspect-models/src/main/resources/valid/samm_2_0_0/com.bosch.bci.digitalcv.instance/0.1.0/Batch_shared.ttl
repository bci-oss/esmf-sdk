@prefix : <urn:samm:com.bosch.bci.digitalcv.instance:0.1.0#>.
@prefix samm: <urn:samm:org.eclipse.esmf.samm:meta-model:2.0.0#> .
@prefix samm-c: <urn:samm:org.eclipse.esmf.samm:characteristic:2.0.0#> .
@prefix samm-e: <urn:samm:org.eclipse.esmf.samm:entity:2.0.0#> .
@prefix unit: <urn:samm:org.eclipse.esmf.samm:unit:2.0.0#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:batchNumber a samm:Property ;
    samm:preferredName "Batch Number"@en ;
    samm:preferredName "Chargennummer"@de ;
    samm:description "The identification of the batch on the MES batch identifier."@en ;
    samm:characteristic [
        a samm-c:Trait ;
        samm-c:baseCharacteristic [
            a samm-c:Code ;
            samm:preferredName "Batch ID"@en ;
            samm:dataType xsd:string
        ] ;
        samm-c:constraint [
            a samm-c:RegularExpressionConstraint ;
            samm:value "[A-Z0-9_-]+"
        ]
    ] .

:batchDescription a samm:Property ;
    samm:preferredName "Batch Description"@en ;
    samm:preferredName "Chargenbeschreibung"@de ;
    samm:description "Description of the batch."@en ;
    samm:characteristic samm-c:Text .

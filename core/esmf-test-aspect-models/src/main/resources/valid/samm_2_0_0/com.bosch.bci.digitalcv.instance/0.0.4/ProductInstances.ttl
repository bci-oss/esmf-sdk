@prefix : <urn:bamm:com.bosch.bci.digitalcv.instance:0.0.4#>.
@prefix bamm: <urn:bamm:io.openmanufacturing:meta-model:2.0.0#> .
@prefix bamm-c: <urn:bamm:io.openmanufacturing:characteristic:2.0.0#> .
@prefix bamm-e: <urn:bamm:io.openmanufacturing:entity:2.0.0#> .
@prefix unit: <urn:bamm:io.openmanufacturing:unit:2.0.0#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:ProductInstances a bamm:Aspect ;
    bamm:preferredName "Product Instances"@en ;
    bamm:preferredName "Produktinstanzen"@de ;
    bamm:description "Aspect for product instances of a certain product type."@en ;
    bamm:description "Aspekt welcher die Produktinstanzen eines gewissen Produkttyps liefert"@de ;
    bamm:properties ( :items ) ;
    bamm:operations ( ) .

:items a bamm:Property ;
    bamm:preferredName "Product Instances"@en ;
    bamm:preferredName "Produktinstanzen"@de ;
    bamm:description "A list of product instances sorted in chronological order."@en ;
    bamm:description "Eine Liste mit chronologisch sortierten Produktinstanzen."@de ;
    bamm:characteristic [
        a bamm-c:SortedSet ;
        bamm:dataType :ProductInstance
    ] .
#
# Aspect model for product
# format turtle https://www.w3.org/TeamSubmission/turtle
#
@prefix :       <urn:bamm:com.bosch.bci.digitalcv.instance:0.0.2#>.
@prefix bamm:   <urn:bamm:io.openmanufacturing:meta-model:1.0.0#> .
@prefix bamm-c: <urn:bamm:io.openmanufacturing:characteristic:1.0.0#> .
@prefix bamm-e: <urn:bamm:io.openmanufacturing:entity:1.0.0#> .
@prefix unit:   <urn:bamm:io.openmanufacturing:unit:1.0.0#> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .

:ProcessedPart
    a                  bamm:Entity ;
    bamm:name          "ProcessedPart" ;
    bamm:preferredName "ProcessedPart"@en ;
    bamm:preferredName "Verarbeites Teil"@de ;
    bamm:description   "Definition of a part processed in manufacturing."@en ;
    bamm:properties    ( <urn:bamm:com.bosch.bci.digitalcv.type:0.0.2#materialNumber>
                         <urn:bamm:com.bosch.bci.digitalcv.type:0.0.2#materialDescription>
                         <urn:bamm:com.bosch.bci.digitalcv.type:0.0.2#materialClass>
                         [ bamm:property <urn:bamm:com.bosch.bci.digitalcv.type:0.0.2#materialClassDescription>; bamm:optional true ]
                         [ bamm:property <urn:bamm:com.bosch.bci.digitalcv.instance:0.0.2#batchNumber>; bamm:optional true ]
                         [ bamm:property <urn:bamm:com.bosch.bci.digitalcv.instance:0.0.2#batchDescription>; bamm:optional true ]
                         :uniquePartId
                         :productionTime
                         [ bamm:property :serialNumber; bamm:optional true ]
                         [ bamm:property :isAssembledFrom; bamm:optional true ] ) .

:uniquePartId
    a                   bamm:Property ;
    bamm:name           "uniquePartid" ;
    bamm:preferredName  "Unique Part ID"@en ;
    bamm:preferredName  "Teilenummer"@de ;
    bamm:description    "The identification of the processed part based on the MES part number."@en ;
    bamm:exampleValue   "F03Z1234560213107000283011234567990" ;
    bamm:characteristic :PartNumber .

:PartNumber
    a                         bamm-c:Trait ;
    bamm:name                 "PartNumber" ;
    bamm:see                  <https://inside-docupedia.bosch.com/confluence/x/ssq0O> ;
    bamm-c:baseCharacteristic [ a                  bamm-c:Code ;
                                bamm:name          "PartNumberCode" ;
                                bamm:preferredName "Part Number"@en ;
                                bamm:dataType      xsd:string ] ;
    bamm-c:constraint         [ a          bamm-c:RegularExpressionConstraint ;
                                bamm:name  "PartNumberPattern" ;
                                bamm:value "[A-Z][A-Z0-9]{34}" ] .

:SerialNumber
    a                 bamm-c:Trait ;
    bamm:name         "SerialNumber" ;
    bamm-c:baseCharacteristic [ a bamm-c:Code ;
                                bamm:name          "SerialNumberCode" ;
                                bamm:preferredName "Serial Number"@en ;
                                bamm:dataType xsd:string
                      ] ;
    bamm-c:constraint [ a                         bamm-c:RegularExpressionConstraint ;
                        bamm:name                 "SerialNumberPattern" ;
                        bamm:value                "[A-Z0-9]{10,12}" ] .

:serialNumber
    a                   bamm:Property ;
    bamm:name           "serialNumber" ;
    bamm:preferredName  "Serial Number"@en ;
    bamm:preferredName  "Seriennummer"@de ;
    bamm:description    "The identification of the processed part based on a externally valid structured identifier."@en ;
    bamm:characteristic :SerialNumber .

:productionTime
    a                   bamm:Property ;
    bamm:name           "productionTime" ;
    bamm:preferredName  "Production Date and Time"@en ;
    bamm:description    "Date and time, when the part was produced"@en ;
    bamm:characteristic bamm-c:Timestamp .

:isAssembledFrom
    a                   bamm:Property ;
    bamm:name           "isAssembledFrom" ;
    bamm:preferredName  "Is Assembled From"@en ;
    bamm:description    "A list of processed parts this part is assembled from."@en ;
    bamm:characteristic [ a             bamm-c:Set ;
                          bamm:name     "PartNumbers" ;
                          bamm-c:elementCharacteristic :PartNumber ] .


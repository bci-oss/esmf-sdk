:Parts a bamm:Aspect ;
    bamm:preferredName "Part Bill of Material"@en ;
    bamm:description "Aspect for bill of material for processed parts."@en ;
    bamm:properties ( :items ) ;
    bamm:operations ( ) .

:items a bamm:Property ;
    bamm:preferredName "Processed Parts"@en ;
    bamm:preferredName "Teile"@de ;

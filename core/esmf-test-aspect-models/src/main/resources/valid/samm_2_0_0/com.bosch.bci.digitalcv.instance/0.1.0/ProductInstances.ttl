@prefix : <urn:samm:com.bosch.bci.digitalcv.instance:0.1.0#> .
@prefix samm: <urn:samm:org.eclipse.esmf.samm:meta-model:2.0.0#> .
@prefix samm-c: <urn:samm:org.eclipse.esmf.samm:characteristic:2.0.0#> .
@prefix samm-e: <urn:samm:org.eclipse.esmf.samm:entity:2.0.0#> .
@prefix unit: <urn:samm:org.eclipse.esmf.samm:unit:2.0.0#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:ProductInstances a samm:Aspect ;
    samm:preferredName "Product Instances"@en ;
    samm:preferredName "Produktinstanzen"@de ;
    samm:description "Aspect for product instances of a certain product type."@en ;
    samm:description "Aspekt welcher die Produktinstanzen eines gewissen Produkttyps liefert"@de ;
    samm:properties ( :items ) ;
    samm:operations ( ) .

:items a samm:Property ;
    samm:preferredName "Product Instances"@en ;
    samm:preferredName "Produktinstanzen"@de ;
    samm:description "A list of product instances sorted in chronological order."@en ;
    samm:description "Eine Liste mit chronologisch sortierten Produktinstanzen."@de ;
    samm:characteristic [
        a samm-c:SortedSet ;
        samm:dataType :ProductInstance
    ] .
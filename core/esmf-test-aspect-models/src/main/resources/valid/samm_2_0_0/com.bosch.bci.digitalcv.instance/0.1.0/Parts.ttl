:Parts a samm:Aspect ;
    samm:preferredName "Part Bill of Material"@en ;
    samm:description "Aspect for bill of material for processed parts."@en ;
    samm:properties ( :items ) ;
    samm:operations ( ) .

:items a samm:Property ;
    samm:preferredName "Processed Parts"@en ;
    samm:preferredName "Teile"@de ;
    samm:description "A list of processed parts sorted in chronological order."@en ;
    samm:description "Eine Liste mit chronologisch sortierten Teilen."@de ;
    samm:characteristic [
        a samm-c:SortedSet ;
        samm:dataType :ProcessedPart
    ] .
:Countries
    a                  samm:Aspect ;
    samm:name          "Countries" ;
    samm:preferredName "Countries"@en ;
    samm:description   "Aspect for countries."@en ;
    samm:properties    ( :items ) ;
    samm:operations    ( ) .

:items
    a                   samm:Property ;
    samm:name           "items" ;
    samm:preferredName  "Country List"@en ;
    samm:description    "A list of countries."@en ;
    samm:characteristic [ a             samm-c:SortedSet ;
                          samm:name     "Countries" ;
                          samm:dataType :Country ] .

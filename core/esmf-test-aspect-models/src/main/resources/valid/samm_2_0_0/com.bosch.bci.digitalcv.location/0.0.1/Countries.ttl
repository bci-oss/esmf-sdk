:Countries
    a                  bamm:Aspect ;
    bamm:name          "Countries" ;
    bamm:preferredName "Countries"@en ;
    bamm:description   "Aspect for countries."@en ;
    bamm:properties    ( :items ) ;
    bamm:operations    ( ) .

:items

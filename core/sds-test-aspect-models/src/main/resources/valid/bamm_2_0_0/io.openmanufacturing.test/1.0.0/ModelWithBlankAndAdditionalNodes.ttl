@prefix : <urn:bamm:io.openmanufacturing.test:1.0.0#> .
@prefix bamm:   <urn:bamm:io.openmanufacturing:meta-model:2.0.0#> .
@prefix bamm-c: <urn:bamm:io.openmanufacturing:characteristic:2.0.0#> .
@prefix aux: <urn:bamm:io.openmanufacturing:aux#> .
@prefix xsd:    <http://www.w3.org/2001/XMLSchema#> .

[
  aux:contains  :ModelWithBlankAndAdditionalNodes, :testProperty, :NumberList, :Number, _:blankNode ;
] .

:ModelWithBlankAndAdditionalNodes a bamm:Aspect ;
    bamm:properties  ( :testProperty ) .

:testProperty a bamm:Property ;
    bamm:characteristic :NumberList .

:NumberList a bamm-c:List ;
    bamm-c:elementCharacteristic _:blankNode .

_:blankNode a bamm-c:Trait ;
    bamm-c:baseCharacteristic :Number ;
    bamm-c:constraint [ a bamm-c:RangeConstraint ;
                        bamm-c:minValue  "5"^^xsd:nonNegativeInteger ;
                        bamm-c:maxValue  "10"^^xsd:nonNegativeInteger ; ] .

:Number a bamm:Characteristic ;
    bamm:dataType xsd:nonNegativeInteger .

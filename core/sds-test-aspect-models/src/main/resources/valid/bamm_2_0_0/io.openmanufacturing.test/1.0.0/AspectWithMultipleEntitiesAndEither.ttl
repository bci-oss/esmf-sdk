@prefix : <urn:bamm:io.openmanufacturing.test:1.0.0#> .
@prefix bamm: <urn:bamm:io.openmanufacturing:meta-model:2.0.0#> .
@prefix bamm-c: <urn:bamm:io.openmanufacturing:characteristic:2.0.0#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:AspectWithMultipleEntitiesAndEither a bamm:Aspect ;
   bamm:properties ( :testEntityOne :testEntityTwo :testEitherProperty ) ;
   bamm:operations ( ) .

:testEntityOne a bamm:Property ;
   bamm:characteristic :TestEntityCharacteristic .

:testEntityTwo a bamm:Property ;
   bamm:characteristic :TestEntityCharacteristic .

:testEitherProperty a bamm:Property ;
   bamm:characteristic :TestEither .

:TestEntity a bamm:Entity ;
   bamm:properties ( :testString :testInt :testFloat :testLocalDateTime :randomValue ) .

:testString a bamm:Property ;
   bamm:exampleValue "Example Value Test" ;
   bamm:characteristic bamm-c:Text .

:testInt a bamm:Property ;
   bamm:exampleValue "3"^^xsd:int ;
   bamm:characteristic :Int .

:testFloat a bamm:Property ;
   bamm:exampleValue "2.25"^^xsd:float ;
   bamm:characteristic :Float .

:testLocalDateTime a bamm:Property ;
   bamm:exampleValue "2018-02-28T14:23:32.918Z"^^xsd:dateTimeStamp ;
   bamm:characteristic :LocalDateTime .

:randomValue a bamm:Property ;
   bamm:characteristic bamm-c:Text .

:Int a bamm:Characteristic ;
   bamm:dataType xsd:int .

:Float a bamm:Characteristic ;
   bamm:dataType xsd:float .

:LocalDateTime a bamm:Characteristic ;
   bamm:dataType xsd:dateTimeStamp .

:TestEntityCharacteristic a bamm:Characteristic ;
   bamm:dataType :TestEntity .

:TestEither a bamm-c:Either ;
   bamm:preferredName "Test Either"@en ;
   bamm:description "This is a test Either."@en ;
   bamm:see <http://example.com/omp> ;
   bamm-c:right :RightEitherType ;
   bamm-c:left :LeftEitherType .

:RightEitherType a bamm:Characteristic ;
   bamm:preferredName "Right either type"@en ;
   bamm:description "Right type Characteristic"@en ;
   bamm:see <http://example.com/omp> ;
   bamm:dataType :TestEntity .

:LeftEitherType a bamm:Characteristic ;
   bamm:preferredName "Left either type"@en ;
   bamm:description "Left type Characteristic"@en ;
   bamm:see <http://example.com/omp> ;
   bamm:dataType :TestEntity .

